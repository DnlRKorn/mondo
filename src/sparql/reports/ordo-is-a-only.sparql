PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix IAO: <http://purl.obolibrary.org/obo/IAO_>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#>
prefix def: <http://purl.obolibrary.org/obo/IAO_0000115>
prefix owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT distinct ?term ?label

WHERE 
{
  	?term rdfs:subClassOf* <http://purl.obolibrary.org/obo/MONDO_0000001> ;
         rdfs:label ?label ;
        oboInOwl:hasDbXref ?xref ;
        rdfs:subClassOf ?super .
  	?child rdfs:subClassOf* ?term .
  
  FILTER(?child!=?term)
 
  
  FILTER(STRSTARTS(str(?xref), "Orphanet:"))
  
  FILTER NOT EXISTS {
  	?term  oboInOwl:hasDbXref ?xref2 .
    FILTER(!STRSTARTS(str(?xref2), "Orphanet:"))
  }
  FILTER( !isBlank(?term))
} 
